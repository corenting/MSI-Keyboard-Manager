        <Label x:Name="LabelMode" Content="Mode:" Margin="10,10,0,0" HorizontalAlignment="Left" Width="43" RenderTransformOrigin="0.513,1.405" Height="26" VerticalAlignment="Top"/>
        <ComboBox x:Name="ComboBoxMode" Margin="58,10,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="450"/>
    </Grid>
</Window>
